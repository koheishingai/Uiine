doctype html
html
  head
    meta(charset="utf-8")
    title= "Sync"
    meta(http-equiv="X-UA-Compatible", content="IE=EmulateIE7, IE=9")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    link(rel="stylesheet", href="./styles/main.css")
  body
    section.wrap
      header
        div.menu.about
        div.menu.search
        div.menu.share
      iframe.play_ground(src="")
      div.main
        div.shadow.fade
          img.logo.fade(src="./images/logo.png")
          h2.logo.fade(data='main.sub-title')
                                         span 
                                           a(href="http://www.skrillex.com/" target="_blank")= "Skrillex"
                                         |  + 
                                         span
                                           a(href="https://www.youtube.com/user/fracionado" target="_blank")= "Fracionado"
          // a.fade(href="/skrillex")
          img.logo-play(src="./images/play.svg")
          img.logo-more(src="./images/more.svg")
          audio#logo-audio.logo(src="./sounds/logo-wrap.mp3", controls, preload)
      div.bottom
        div.main
          div.shadow.fade
            img.logo.fade(src="./images/logo.png")
            h2.logo.fade(data='main.sub-title')
                                           span
                                             a(href="http://taylorswift.com/" target="_blank")= "Taylor Swift"
                                           |  +  
                                           span
                                             a(href="https://www.youtube.com/user/TannerPatrick" target="_blank")= "Tanner Patrick"
            // a.fade(href="/taylor_swift")
            img.logo-play.fade(src="./images/play.svg")
            img.logo-more(src="./images/more.svg")
            // audio#logo-audio.logo(src="./sounds/logo-wrap.mp3", controls, preload)
        div.bottom_menu
          div.menu.phone
            div.shadow.fade
              // p= "About Sync"
              img.bottom_logo.fade(src="./images/about.png")
          div.menu.glasses.edge
            div.shadow.fade
              // p= "Try Sync"
              img.bottom_logo.fade(src="./images/try.png")
      div.controller
        div.close.top
      script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")
      script(src="./scripts/math.min.js")
      script(src="./scripts/lang.js")
      script(src="./scripts/main.js")
      // script(src="http://api.html5media.info/1.1.8/html5media.min.js")
      // script(src="./scripts/video.js")